{
  "name": "rpi-tools-cron",
  "version": "1.0.0",
  "description": "Raspberry Pi 3 Cron",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "repository": "k03mad/rpi-tools-cron",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "app-root-path": "2.2.1",
    "node-cron": "2.0.3",
    "node-routeros": "1.6.2",
    "oui": "10.0.9",
    "pm2": "3.5.1",
    "require-all": "3.0.0",
    "utils-mad": "1.18.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-k03mad": "2.5.3",
    "eslint-plugin-const-case": "1.2.2",
    "eslint-plugin-jsdoc": "10.2.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-optimize-regex": "1.1.6",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sonarjs": "0.4.0",
    "eslint-plugin-sort-requires": "2.1.0",
    "eslint-plugin-unicorn": "9.1.1",
    "pre-commit": "1.2.2"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules || true",
    "clean:log": "rm -rf ./pm2.log || true",
    "lint": "eslint --report-unused-disable-directives ./ --format codeframe && echo '\n\\033[34mESLinted!\\033[39m\n'",
    "setup": "npm i",
    "start": "npm run clean:log && pm2 start app/cron.js -l ./pm2.log --watch --watch-delay 120",
    "task": "node runner.js"
  },
  "pre-commit": [
    "lint"
  ],
  "license": "Unlicense"
}
